{
  "name": "test-app",
  "version": "0.1.0",
  "author": "Piotr",
  "description": "A JupyterLab based standalone application",
  "files": [
    "lib/**/*.{d.ts,js.map,js}",
    "style/**/*.{css,svg}",
    "schema/*.json"
  ],
  "sideEffects": [
    "style/**/*.css"
  ],
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "tsc && webpack",
    "build:prod": "tsc && webpack --mode=production",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "prepublishOnly": "yarn run build",
    "prettier": "prettier --write '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'",
    "prettier:check": "prettier --list-different \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "lint": "yarn prettier && yarn eslint",
    "lint:check": "yarn prettier:check && yarn eslint:check",
    "test": "jest",
    "watch:ts": "tsc -w --listEmittedFiles",
    "watch:webpack": "webpack --watch",
    "watch": "npm-run-all --parallel watch:ts watch:webpack"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^6.0.10",
    "@jupyter-widgets/jupyterlab-manager": "^5.0.13",
    "@jupyter/web-components": "^0.15.0",
    "@jupyterlab/application": "^4.0.0",
    "@jupyterlab/apputils": "^4.0.0",
    "@jupyterlab/codemirror": "^4.0.0",
    "@jupyterlab/docmanager": "^4.0.0",
    "@jupyterlab/docregistry": "^4.0.0",
    "@jupyterlab/markedparser-extension": "^4.0.0",
    "@jupyterlab/notebook": "^4.0.0",
    "@jupyterlab/rendermime": "^4.0.0",
    "@jupyterlab/services": "^7.0.0",
    "@jupyterlab/theme-light-extension": "^4.0.0",
    "@jupyterlab/ui-components": "^4.0.0",
    "@lumino/commands": "^2.2.0",
    "@lumino/widgets": "^2.3.1",
    "@mljar/mercuryextension": "link:../mercury-extension/",
    "es6-promise": "~4.2.8"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.17.10",
    "@jupyterlab/testutils": "^4.0.0",
    "@types/jest": "^26.0.10",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "css-loader": "~3.2.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsdoc": "^30.6.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "~5.0.2",
    "fs-extra": "^8.1.0",
    "glob": "~7.1.6",
    "jest": "^26.4.2",
    "jest-raw-loader": "^1.0.1",
    "mini-css-extract-plugin": "~0.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "raw-loader": "~4.0.0",
    "rimraf": "~3.0.2",
    "style-loader": "~1.0.1",
    "svg-url-loader": "~6.0.0",
    "ts-jest": "^26.3.0",
    "typescript": "~5.0.2",
    "url-loader": "~4.1.1",
    "watch": "~1.0.2",
    "webpack": "^5.7.0",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-cli": "^4.2.0",
    "whatwg-fetch": "^3.0.0"
  }
}
